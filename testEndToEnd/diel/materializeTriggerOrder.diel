create event table t1 (a int);
create view v0 as select a from t1;


create view viewB as select a+1 as a from v0, t2 where v0.a = t2.a;
create view viewA as select a+1 as a from viewB; 

create output o3 as select viewB.a from viewB;
create output o1 as select viewA.a from viewA;
create output o2 as select viewA.a from viewA;
