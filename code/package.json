{
  "name": "diel",
  "version": "1.0.0",
  "description": "data interaction events log is a new SQL embeded DSL for managing the state of history-aware interactive applications",
  "bin": {
    "diel-cli": "dist/bin/compile.js"
  },
  "scripts": {
    "build": "tsc",
    "test-cli": "ts-node --no-cache src/bin/compile.ts",
    "lang": "antlr4ts -visitor -no-listener src/parser/grammar/DIEL.g4",
    "test": "ts-node --no-cache src/tests/compilerTest.ts",
    "probeTest": "ts-node --no-cache src/tests/probeTest.ts",
    "testFilesSetup": "ts-node --no-cache src/tests/testFilesSetup.ts",
    "testFiles2": "ts-node --no-cache src/tests/testFiles2.ts",
    "testFilesXFilter": "ts-node --no-cache src/tests/testFilesXFilter.ts",
    "testFilesSetupOnline": "node --inspect-brk -r ts-node/register src/tests/testFilesSetup.ts",
    "testFilesXFilterOnline": "node --inspect-brk -r ts-node/register src/tests/testFilesXFilter.ts",
    "debug": "node --inspect-brk -r ts-node/register src/tests/compilerTest.ts",
    "token": "cp src/parser/grammar/DIEL.g4 src/debug/ && cd src/debug && echo ./antlr4-tester.sh DIEL.g4 \"<your query>\" queries"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yifanwu/diel.git"
  },
  "keywords": [
    "interaction",
    "history",
    "sql"
  ],
  "author": "yifan wu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yifanwu/diel/issues"
  },
  "homepage": "https://github.com/yifanwu/diel#readme",
  "devDependencies": {
    "antlr4ts-cli": "^0.4.0-alpha.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/glob": "^7.1.1",
    "@types/sql.js": "0.0.29",
    "antlr4ts": "^0.4.1-alpha.0",
    "commander": "^2.19.0",
    "glob": "^7.1.3",
    "sql.js": "^0.5.0"
  }
}
